# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token
DISCORD_GUILD_ID=your_guild_id
BOT_NAME=Requiem Bot

# Role IDs
ADMIN_ROLE_IDS=role_id1,role_id2  # Comma-separated list of Admin role IDs
OFFICER_ROLE_IDS=role_id1,role_id2  # Comma-separated list of Officer role IDs
CLAN1_ROLE_ID=clan1_role_id
CLAN2_ROLE_ID=clan2_role_id

# Additional Role IDs (comma-separated, if multiple)
CLAN1_ADDITIONAL_ROLES=123456789,987654321
CLAN2_ADDITIONAL_ROLES=123456789,987654321

# Clan Names
CLAN1_NAME=Clan 1
CLAN2_NAME=Clan 2
CLAN1_ALIASES=clan1,c1
CLAN2_ALIASES=clan2,c2

# Database Configuration
DB_HOST=db
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=your_db_password

# API Configuration
API_PORT=3000
API_SECRET_KEY=your_api_secret_key

# SSL Configuration
SSL_KEYFILE=ssl/privkey.pem
SSL_CERTFILE=ssl/fullchain.pem

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/bot.log 

# RaidHelper Configuration
RAIDHELPER_SERVER_ID=your_discord_server_id
RAIDHELPER_API_KEY=your_raidhelper_api_key
